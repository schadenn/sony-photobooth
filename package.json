{
  "name": "sony-photobooth",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "cra-template": "1.1.2",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "fast-xml-parser": "^3.20.3",
